{
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:jest-dom/recommended",
    "prettier"
  ],
  "plugins": ["jest-dom"],
  "rules": {
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "hrefLeft", "hrefRight" ],
      "aspects": [ "invalidHref", "preferButton" ]
    }]
  }
}
